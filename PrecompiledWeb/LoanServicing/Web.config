			<authorization>
				<allow roles="Borrower"/>
				<deny users="*"/>
			</authorization>
		</system.web>
	</location>
	<location path="Roles/Investors">
		<system.web>
			<authorization>
				<allow roles="Investor"/>
				<deny users="*"/>
			</authorization>
		</system.web>
