			</providers>
		</membership>
		<roleManager enabled="true" defaultProvider="CustomRole" cacheRolesInCookie="false">
			<providers>
				<clear/>
				<add name="CustomRole" type="CustomProviders.CustomRoleProvider" connectionStringName="sqlData"/>
